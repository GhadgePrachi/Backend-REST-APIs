{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "facilities.response.json",
	"title": "Facility",
	"description": "a list of facilities details",
	"type": "array",
	"items": {
        "type": "object",
		"properties": {
			"facility_id": {
				"type": "integer"
			},
			"facility_name": {
				"type": "string"
			},
			"address": {
				"type": "string"
			},
			"telephone": {
				"type": "string"
			},
			"timings": {
				"type": "string"
			},
			"ratings": {
				"type": "string"
			}
		}
    },
    "uniqueItems": true,
	"required": [ "facility_id"]
}