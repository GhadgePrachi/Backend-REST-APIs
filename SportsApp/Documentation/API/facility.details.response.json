{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "facility.details.json",
	"title": "Facility",
	"description": "a facility's details",
	"type": "object",
	"properties": {
		"facility_id": {
			"type": "integer"
		},
		"facility_name": {
			"type": "string"
		},
		"category": {
			"type": "string"
		},
		"address": {
			"type": "string"
		},
		"telephone": {
			"type": "string"
		},
		"timings": {
			"type": "string"
		},
		"ratings": {
			"type": "string"
		},
		"summary": {
			"type": "string"
		},
		"image_url": {
			"type": "string"
		}
	},
	"required": [ "facility_id"]
}